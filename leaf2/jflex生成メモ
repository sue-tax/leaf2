	int nextToken() {
		try {
			token = yylex();
			if (token == EOF) {
//				D.dprint("+++EOF+++");
				return token = ENDINPUT;
			}
			return token;
		} catch(Exception e) {
			return token = ENDINPUT;
		}
	}



